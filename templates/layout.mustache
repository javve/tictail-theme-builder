var navigation = $('.navigation');
$('.burger').click(function(e) {
    e.preventDefault();
    if ($(window).width() < 575) {
        if (navigation.is(':visible')) {
            navigation.slideUp();
        } else {
            navigation.slideDown();
        }
    } else {
        if (navigation.hasClass('open')) {
            navigation.removeClass('open');
            $('body').removeClass('navigation-open');
        } else {
            navigation.addClass('open');
            $('body').addClass('navigation-open');
        }
    }
    return false;
});

var searchBox = $('.search-box');
$('.search-icon').click(function(e) {
    e.preventDefault();
    if (searchBox.hasClass('open')) {
        searchBox.removeClass('open');
    } else {
        searchBox.addClass('open');
    }
    return false;
});

var setProductListWidth = function() {
    var totalWidth = 0;
    $('.product_list li').each(function() {
        totalWidth += $(this).outerWidth();
    });
    $('.product_list').width(totalWidth);
    $('.content_container').width($('.product_list').outerWidth(true));
    $('body').width(totalWidth+30);
};

var setProductPageWidth = function() {
    var totalWidth = 0,
        productsImagesWidth = 0,
        relatedProductsWidth = 0;

    $('.product_images img').each(function() {
        productsImagesWidth += $(this).outerWidth(true);
    });

    relatedProductsWidth += $('.related_products_title').outerWidth(true);
    $('.product_list li').each(function() {
        relatedProductsWidth += $(this).width();
    });
    $('.related_products').width(relatedProductsWidth+30);

    totalWidth += $('.product_info').outerWidth(true);
    totalWidth += productsImagesWidth;
    totalWidth += relatedProductsWidth;
    $('.content_container').width(totalWidth+40);
    $('body').width(totalWidth+30);
};

var calculateProductPageColumns = function() {
    var columns = 1;
    while ($('.product_info').height() > $('.content_container').height() && columns < 5) {
        columns++;
        $('.product_info').addClass('column'+columns);
    }
};

var setBodySize = function() {
    $('body').height($('html').height() - 20 - parseInt($('body').css('padding-top'), 10));
};

var initLightbox = function() {
    $('.product_images a, .product_image_primary_link').magnificPopup({
        type: 'image',
        gallery:{
            enabled:true
        }
    });
};

if ($(window).width() > 575) {
    // http://adomas.org/javascript-mouse-wheel/
    (function(e){function r(t){var n=t||window.event,r=[].slice.call(arguments,1),i=0,s=true,o=0,u=0;t=e.event.fix(n);t.type="mousewheel";if(n.wheelDelta){i=n.wheelDelta/120}if(n.detail){i=-n.detail/3}u=i;if(n.axis!==undefined&&n.axis===n.HORIZONTAL_AXIS){u=0;o=-1*i}if(n.wheelDeltaY!==undefined){u=n.wheelDeltaY/120}if(n.wheelDeltaX!==undefined){o=-1*n.wheelDeltaX/120}r.unshift(t,i,o,u);return(e.event.dispatch||e.event.handle).apply(this,r)}var t=["DOMMouseScroll","mousewheel"];if(e.event.fixHooks){for(var n=t.length;n;){e.event.fixHooks[t[--n]]=e.event.mouseHooks}}e.event.special.mousewheel={setup:function(){if(this.addEventListener){for(var e=t.length;e;){this.addEventListener(t[--e],r,false)}}else{this.onmousewheel=r}},teardown:function(){if(this.removeEventListener){for(var e=t.length;e;){this.removeEventListener(t[--e],r,false)}}else{this.onmousewheel=null}}};e.fn.extend({mousewheel:function(e){return e?this.bind("mousewheel",e):this.trigger("mousewheel")},unmousewheel:function(e){return this.unbind("mousewheel",e)}})})(jQuery)

    $("html, body, *").mousewheel(function(event, delta) {
        this.scrollLeft -= (delta * 30);
        event.preventDefault();
    });

    function adjustSizes() {
        if ($('#products').size() > 0) {
            setProductListWidth();
        }
        if ($('#product').size() > 0) {
            calculateProductPageColumns();
            setProductPageWidth();
        }
        setBodySize();
    }
    $(window).resize(adjustSizes);
    $(window).load(adjustSizes);
}

$(document).ready(initLightbox());

</script>

